{
  "name": "croquet-sdk",
  "version": "0.3.4-20",
  "description": "Croquet SDK Documentation",
  "main": "sdk.js",
  "private": true,
  "scripts": {
    "start": "npm-watch",
    "build": "jsdoc -c jsdoc.json -d build",
    "deploy": "./deploy.sh",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        ".",
        "../teatime"
      ],
      "ignore": [
        "build"
      ],
      "extensions": "js,json,html,css,tmpl,md",
      "quiet": false
    }
  },
  "author": "Croquet Corporation",
  "license": "UNLICENSED",
  "dependencies": {
    "jsdoc": "^3.6.6",
    "moment": "^2.29.1",
    "npm-watch": "^0.7.0",
    "parcel": "^1.12.4",
    "taffydb": "^2.7.3"
  }
}
