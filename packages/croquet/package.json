{
  "name": "@croquet/croquet",
  "version": "0.3.3-8",
  "description": "",
  "main": "pub/croquet-croquet.js",
  "types": "types.d.ts",
  "//": "/* files listed explicitly so sourcemap does not get published */",
  "files": [
    "pub/croquet-croquet.js",
    "types.d.ts"
  ],
  "scripts": {
    "start": "npm run watch",
    "watch": "npm-watch build-dev",
    "build-dev": "rollup -c --environment NODE_ENV:development",
    "build-prod": "rollup -c --environment NODE_ENV:production",
    "prepublishOnly": "npm run build-prod",
    "clean": "rm -f pub/*; git checkout pub/",
    "prepare": ""
  },
  "watch": {
    "build-dev": {
      "patterns": [
        ".",
        "../../../teatime",
        "../../../util",
        "../../../math"
      ],
      "ignore": "./pub",
      "extensions": "js,json",
      "quiet": false
    }
  },
  "author": "Croquet Corporation",
  "license": "UNLICENSED",
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "fast-json-stable-stringify": "^2.1.0",
    "fastpriorityqueue": "^0.6.3",
    "pako": "^1.0.11",
    "seedrandom": "^3.0.5",
    "toastify-js": "^1.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "dotenv-flow": "^3.2.0",
    "magic-string": "^0.25.7",
    "moment": "^2.27.0",
    "npm-watch": "^0.7.0",
    "rollup": "^2.23.1",
    "rollup-plugin-license": "^2.2.0",
    "rollup-plugin-terser": "^7.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
