{
  "name": "@croquet/croquet",
  "version": "0.3.2-0",
  "description": "",
  "main": "dist/croquet.min.js",
  "types": "types.d.ts",
  "//": "/* files listed explicitly so sourcemap does not get published */",
  "files": [
    "dist/croquet.min.js",
    "types.d.ts"
  ],
  "scripts": {
    "start": "npm run watch",
    "watch": "npm-watch build",
    "build": "rollup -c --environment NODE_ENV:development",
    "prepublish": "rollup -c --environment NODE_ENV:production"
  },
  "watch": {
    "build": {
      "patterns": [
        ".",
        "../../../teatime",
        "../../../util",
        "../../../math"
      ],
      "ignore": "./dist",
      "extensions": "js,json",
      "quiet": false
    }
  },
  "author": "Croquet Corporation",
  "license": "UNLICENSED",
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "fast-json-stable-stringify": "^2.0.0",
    "fastpriorityqueue": "^0.6.1",
    "pako": "^1.0.10",
    "seedrandom": "^2.4.4",
    "toastify-js": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@croquet/teatime": "file:../../../teatime",
    "dotenv-flow": "^3.1.0",
    "magic-string": "^0.25.6",
    "npm-watch": "^0.6.0",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-license": "^2.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
